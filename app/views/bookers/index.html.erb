<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>テスト</td>
      <td>テスト</td>
      <td><a class="show" href="#">Show</a></td>
      <td><a class="edit" href="#">Edit</a></td>
      <td><a class="delete" href="#">Destroy</a></td>
    </tr>
  </tbody>
</table>

<h2>New book</h2>
<%= form_with model: @book, url:'/bookers', local:true do |f| %>
  <div class="title">
    <label>title</label>
    <%= f.text_field :title %>
  </div>
  <div class="body">
    <label>Body</label>
    <%= f.text_area :body %>
  </div>  
  <div class="create">
    <%= f.submit 'Create Book' %>
  </div>  
<% end %>